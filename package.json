{
  "name": "readlearn",
  "version": "1.0.0",
  "description": "A web app that takes in a text in another language, allowing the user to click words they don't know. Outputs a list of words to study later!",
  "private": "true",
  "homepage": "https://chiazo.github.io/read-learn",
  "files": [
    "/public/"
  ],
  "scripts": {
    "test": "this works!",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "build": "webpack",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "language",
    "learning",
    "read"
  ],
  "author": "Chiazo",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "lite-server": "^2.5.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.1",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.11",
    "typescript-bundle": "^1.0.14"
  }
}
